<!DOCTYPE html>
<html lang="en-us" id="extr-page">
<head>
<meta charset="UTF-8">
<link rel="stylesheet" type="text/css" media="screen" href="../../css/bootstrap.min.css">
<link rel="stylesheet" href="../../layui/css/layui.css" media="all" />
</head>
<body>
	<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
		<form class="form-horizontal" onsubmit="return false" id="form">
			<fieldset>
			<br/>
				<div id="all">
					<span style="font-weight:bold;">借款人信息</span>
					<hr class="layui-bg-green"></hr>
					<div class="form-group">
						<label class="col-sm-2 control-label">借款人:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="enterpriseName"></span>
						</div>
						<label class="col-sm-2 control-label">借款人手机号码:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="businessPhone"></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">所属行业:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="industry"></span>
						</div>
						<label class="col-sm-2 control-label">收入情况:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="annualIncome"></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">负债情况:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="isLiabilities"></span>
						</div>
						<label class="col-sm-2 control-label">征信逾期(6月内):</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="creditOverdue"></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">借款用途:</label>
						<div class="col-sm-10" style="width:77%">
							<span class="form-control" id="borrowMoneyUse"></span>
						</div>
					</div>
				</div>
				<div id="qftPropertyDIV">
					<span style="font-weight:bold;">固定资产信息</span>
					<hr class="layui-bg-green"></hr>
					<div class="form-group">
						<label class="col-md-2 control-label">房产证所有权人页:</label>
						<div class="col-md-6" style="width:30%">
							<img id="house" style="margin-top:10px;margin-right:10px;" width='50%' height='50%' />
							<div style="margin-top:10px;margin-right:10px;color:red" >
								<span id="houseSpan">该商家未上传该项资质</span>
							</div>
						</div>
						<label class="col-md-2 control-label">房产证附记页:</label>
						<div class="col-md-6" style="width:30%">
							<img id="houseSubjoin" style="margin-top:10px;margin-right:10px;" width='50%' height='50%'/>
							<div style="margin-top:10px;margin-right:10px;color:red" >
								<span id="houseSubjoinSpan">该商家未上传该项资质</span>
							</div>
						</div>
					</div>
					<div class="form-group">
						<label class="col-md-2 control-label">不动产证所有权人页:</label>
						<div class="col-md-6" style="width:30%">
							<img id="immovables" style="margin-top:10px;margin-right:10px;" width='50%' height='50%' />
							<div style="margin-top:10px;margin-right:10px;color:red" >
								<span id="immovablesSpan">该商家未上传该项资质</span>
							</div>
						</div>
						<label class="col-md-2 control-label">不动产证附记页:</label>
						<div class="col-md-6" style="width:30%">
							<img id="immovablesSubjoin" style="margin-top:10px;margin-right:10px;" width='50%' height='50%'/>
							<div style="margin-top:10px;margin-right:10px;color:red" >
								<span id="immovablesSubjoinSpan">该商家未上传该项资质</span>
							</div>
						</div>
					</div>
				</div>
				
				<div id="qftCarDIV">
					<span style="font-weight:bold;">车辆信息</span>
					<hr class="layui-bg-green"></hr>
					<div class="form-group">
						<label class="col-sm-2 control-label">车辆号码:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="licensePlateNumber"></span>
						</div>
						<label class="col-sm-2 control-label">号牌类型:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="licensePlateNumberType"></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">初始登记日期:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="registerDate"></span>
						</div>
						<label class="col-sm-2 control-label">已行驶公里数:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="driveDistance"></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-2 control-label">车架号:</label>
						<div class="col-sm-10" style="width:30%">
							<span class="form-control" id="carFrameNumber"></span>
						</div>
					</div>
					<div id="otherCar"></div>
				</div>
				
				<div class="form-actions">
					<div class="row" align="center">
						<div class="col-md-12">
							<button class="btn btn-primary" onclick="cancel()">关闭</button>
						</div>
					</div>
				</div>
			</fieldset>
		</form>
	</div>
<script src="../../js/constant.js"></script>
<script type="text/javascript" src="../../js/libs/jquery-3.3.1.min.js"></script>
<script type="text/javascript" src="../../js/jq.js"></script>
<script type="text/javascript" src="../../js/plugin/bootstrapvalidator/bootstrapValidator.min.js"></script>
<script type="text/javascript" src="../../js/common.js"></script>
<script type="text/javascript" src="../../layui/layui.js"></script>
<script type="text/javascript" src="../../js/my/permission.js"></script>
<script type="text/javascript" src="../../js/dateUtil.js"></script>
<script type="text/javascript">
var pers = checkPermission();
layui.use(['form','layer'], function(){
	var layer = layui.layer;
	
	var form=layui.form;
	form.render('select');
	
	initData();
});

function initData(){
	var bidId=getUrlParam("bidId");
	
	$.ajax({
		type : 'get',
		url : domainName + '/api-P2P/qftEnterprises/info',
		data: {bidId:bidId},
		success : function(data) {
			if(data!=null){
				var qftEnterprise = data.qftEnterprise;
				if(qftEnterprise != undefined){
					$("#all").show();
					$("#enterpriseName").html(qftEnterprise.enterpriseName);
					$("#businessPhone").html(qftEnterprise.businessPhone);
					$("#industry").html(qftEnterprise.industry);
					$("#annualIncome").html(qftEnterprise.annualIncome);
					$("#isLiabilities").html(qftEnterprise.isLiabilities);
					$("#creditOverdue").html(qftEnterprise.creditOverdue);
					$("#borrowMoneyUse").html(qftEnterprise.borrowMoneyUse);
					$("#guaranteeCompanyName").html(qftEnterprise.guaranteeCompanyName);
					var suretyBond = qftEnterprise.suretyBond;
					
					if(suretyBond != "" && suretyBond != null){
						$("#suretyBond").show();
						$("#suretyBondSpan").hide();
						var $img="<img style='margin-top:10px;margin-right:10px;' width='25%' height='25%' src="+suretyBond+" />";
						$("#suretyBond").append($img);
					}else{
						$("#suretyBond").hide();
						$("#suretyBondSpan").show();
					}
				}else{
					$("#all").hide();
				}
			}
		}
	});
	
	$.ajax({
		type : 'get',
		url : domainName + '/api-P2P/qftCars/getProperty',
		data: {bidId:bidId},
		success : function(data) {
			if(data!=null){
				var qftProperty = data.qftProperty;
				if(qftProperty == undefined){
					$("#qftPropertyDIV").hide();
				}else{
					var qftPropertyData = qftProperty[0];
					var house = qftPropertyData.house;
					if(house == "" || house == null){
						$("#house").hide();
						$("#houseSpan").show();
					}else{
						$("#house").show();
						$("#house").attr("src",house);
						$("#houseSpan").hide();
					}
					
					var houseSubjoin = qftPropertyData.houseSubjoin;
					if(houseSubjoin == "" || houseSubjoin == null){
						$("#houseSubjoin").hide();
						$("#houseSubjoinSpan").show();
					}else{
						$("#houseSubjoin").show();
						$("#houseSubjoin").attr("src",houseSubjoin);
						$("#houseSubjoinSpan").hide();
					}
					
					var immovables = qftPropertyData.immovables;
					if(immovables == "" || immovables == null){
						$("#immovables").hide();
						$("#immovablesSpan").show();
					}else{
						$("#immovables").show();
						$("#immovables").attr("src",immovables);
						$("#immovablesSpan").hide();
					}
					
					var immovablesSubjoin = qftPropertyData.immovablesSubjoin;
					if(immovablesSubjoin == "" || immovablesSubjoin == null){
						$("#immovablesSubjoin").hide();
						$("#immovablesSubjoinSpan").show();
					}else{
						$("#immovablesSubjoin").show();
						$("#immovablesSubjoin").attr("src",immovablesSubjoin);
						$("#immovablesSubjoinSpan").hide();
					}
				}
				
				var qftCar = data.qftCar;
				if(qftCar == undefined){
					$("#qftCarDIV").hide();
				}else{
					if(qftCar.length>1){
						var licensePlateNumber = qftCar[0].licensePlateNumber;
						$("#licensePlateNumber").html(licensePlateNumber);
						var licensePlateNumberType = qftCar[0].licensePlateNumberType;
						$("#licensePlateNumberType").html(licensePlateNumberType);
						var registerDate = qftCar[0].registerDate;
						$("#registerDate").html(registerDate);
						var driveDistance = qftCar[0].driveDistance;
						$("#driveDistance").html(driveDistance);
						var carFrameNumber = qftCar[0].carFrameNumber;
						$("#carFrameNumber").html(carFrameNumber);
						
						var str = '\
							<span style="font-weight:bold;">车辆信息2</span>\
							<hr class="layui-bg-green"></hr>\
							<div class="form-group">\
								<label class="col-sm-2 control-label">车辆号码:</label>\
							 	<div class="col-sm-10" style="width:30%">\
									<span class="form-control" id="licensePlateNumber1"></span>\
								</div>\
								<label class="col-sm-2 control-label">号牌类型:</label>\
								<div class="col-sm-10" style="width:30%">\
									<span class="form-control" id="licensePlateNumberType1"></span>\
								</div>\
							</div>\
							<div class="form-group">\
								<label class="col-sm-2 control-label">初始登记日期:</label>\
								<div class="col-sm-10" style="width:30%">\
									<span class="form-control" id="registerDate1"></span>\
								</div>\
								<label class="col-sm-2 control-label">已行驶公里数:</label>\
								<div class="col-sm-10" style="width:30%">\
									<span class="form-control" id="driveDistance1"></span>\
								</div>\
							</div>\
							<div class="form-group">\
								<label class="col-sm-2 control-label">车架号:</label>\
								<div class="col-sm-10" style="width:30%">\
									<span class="form-control" id="carFrameNumber1"></span>\
								</div>\
							</div>';
						$("#otherCar").before(str);
						
						var qftCarData = qftCar[1];
						var licensePlateNumber1 = qftCarData.licensePlateNumber;
						$("#licensePlateNumber1").html(licensePlateNumber1);
						var licensePlateNumberType1 = qftCarData.licensePlateNumberType;
						$("#licensePlateNumberType1").html(licensePlateNumberType1);
						var registerDate1 = qftCarData.registerDate;
						$("#registerDate1").html(registerDate1);
						var driveDistance1 = qftCarData.driveDistance;
						$("#driveDistance1").html(driveDistance1);
						var carFrameNumber1 = qftCarData.carFrameNumber;
						$("#carFrameNumber1").html(carFrameNumber1);
					}else{
						var qftCarData = qftCar[0];
						var licensePlateNumber = qftCarData.licensePlateNumber;
						$("#licensePlateNumber").html(licensePlateNumber);
						var licensePlateNumberType = qftCarData.licensePlateNumberType;
						$("#licensePlateNumberType").html(licensePlateNumberType);
						var registerDate = qftCarData.registerDate;
						$("#registerDate").html(registerDate);
						var driveDistance = qftCarData.driveDistance;
						$("#driveDistance").html(driveDistance);
						var carFrameNumber = qftCarData.carFrameNumber;
						$("#carFrameNumber").html(carFrameNumber);
					}
				}
			}
		}
	});
}

function cancel(){
	var index = parent.layer.getFrameIndex(window.name); //先得到当前iframe层的索引
	parent.layer.close(index);
}
</script>
</body>
</html>